<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kersevanivan.CoffeemakerController">
   <children>
      <FlowPane hgap="5.0" prefHeight="102.0" prefWidth="249.0" vgap="5.0">
         <children>
            <Button fx:id="milk" mnemonicParsing="false" onAction="#addMilk" text="Milk" />
            <Button fx:id="sugar" mnemonicParsing="false" onAction="#addSugar" text="Sugar" />
            <Button fx:id="vanilla" mnemonicParsing="false" onAction="#addVanilla" text="Vanilla Flavour" />
            <Separator prefHeight="18.0" prefWidth="226.0" />
            <Button fx:id="coffee" mnemonicParsing="false" onAction="#makeCoffee" prefHeight="26.0" prefWidth="227.0" text="Give me my Coffee" />
            <ProgressBar fx:id="preparationProgress" prefHeight="20.0" prefWidth="227.0" progress="0.0" />
            <TextArea fx:id="state" prefHeight="200.0" prefWidth="227.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
   </children>
</AnchorPane>
